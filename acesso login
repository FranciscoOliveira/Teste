#include <iostream>
#include <cstdlib>

using namespace std;

int main(){

string login;
int senha;
cout <<"Acesse sua conta:";
cout <<"Digite seu login:";
cin>> login;
cout << "digite sua senha:";
cin>> senha;

if((login == "administrador" || login =="ADMINISTRADOR") && senha == 1234)
	cout <<"Bem vindo!!" << endl;
else
cout<<"Dados incorretos, tente novamente!!"<<endl;

return 0;
}
