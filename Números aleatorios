#include <stdio.h>
#include <conio.h>
#include <stdlib.h>
#include <iostream>
 using namespace std;
int main(void)
{
     int i,aleatorios;  
      
     cout<<" Digite a quantidade da valores aleatorios:\n";
     cin>>aleatorios;
     
     cout<<"Gerando "<<aleatorios<<" valores aleatorios:\n ";
          
     for (i = 0; i < aleatorios; i++)
     {
           
           cout<<" "<<int (rand() % 100);
     }
      
     getch();
     return 0;
}
